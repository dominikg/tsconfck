{
	"extends": "@tsconfig/node18",
	"compilerOptions": {
		"types": [
			"foo"
		],
		"strictNullChecks": true,
		"lib": [
			"es2023"
		],
		"module": "node16",
		"target": "es2022",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node16"
	},
	"include": [
		"src/**/*"
	]
}