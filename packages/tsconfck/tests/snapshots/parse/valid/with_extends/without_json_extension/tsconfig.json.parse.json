{
	"extends": "../tsconfig.parent",
	"compilerOptions": {
		"types": [
			"foo"
		],
		"strictNullChecks": true,
		"noImplicitAny": true
	},
	"include": [
		"src/**/*",
		"foo.ts"
	],
	"watchOptions": {
		"watchFile": "useFsEvents",
		"watchDirectory": "useFsEvents",
